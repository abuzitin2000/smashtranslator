{
	"name": "Smash to FGC Translator",
	"description": "Translates smash lingo into fgc ones and vice versa!",
	"version": "1.2",
	"manifest_version": 3,
	"permissions": [
		"activeTab", "scripting"
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "/images/get_started16.png",
			"32": "/images/get_started32.png",
			"48": "/images/get_started48.png",
			"128": "/images/get_started128.png"
		}
	},
	"icons": {
		"16": "/images/get_started16.png",
		"32": "/images/get_started32.png",
		"48": "/images/get_started48.png",
		"128": "/images/get_started128.png"
	}
}